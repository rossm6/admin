{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "airbnb/hooks"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2022
  },
  "rules": {
    "react/jsx-filename-extension": [0],
    "react/react-in-jsx-scope": "off"
    // "max-len": ["error", { "code": 100 }]
  }
}
